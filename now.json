{
  "version": 2,
  "alias": ["blog.fititsolution.com"],
      "github": {
    "autoAlias": false
    },
      "builds": [
    {"src": "server.js", "use": "@now/node-server",
	          "config": {
        "maxLambdaSize": "150mb"
      }
    }
  ],
    "routes": [
      { "src": "(.*\\..*)", "dest": "$1" },
      { "src": ".*", "dest": "/index.html" }
    ]
}

