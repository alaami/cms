{
  "version": 2,
  "alias": ["blog.fititsolution.com"],
      "github": {
    "autoAlias": false
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build"
      }
    ],
    "routes": [
      { "src": "(.*\\..*)", "dest": "$1" },
      { "src": ".*", "dest": "/index.html" }
    ]
}

